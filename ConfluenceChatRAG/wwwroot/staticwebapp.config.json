{
  "routes": [
    {
      "route": "/appsettings.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/css/*", "/js/*", "/_framework/*", "/appsettings.json", "/*.json", "/*.ico", "/*.png", "/*.jpg", "/*.svg"]
  }
}
